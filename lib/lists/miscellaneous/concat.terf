concat nil x_ = x_;
concat (cons x_ y_) nil = cons x_ y_;
concat (cons x_ y_) (cons a_ b_) = cons x_ (concat y_ (cons a_ b_));

# concat takes in two lists and combines their elements to form a single list.

# Examples
# concat (cons 4 (cons 5 nil)) (cons 6 (cons 7 nil)) = cons 4 (cons 5 (cons 6 (cons 7 nil)))
# concat (cons 3 nil) nil = cons 3 nil
