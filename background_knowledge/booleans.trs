.[.[.[if true] x_]  y_] -> x_
.[.[.[if false] x_] y_] -> y_

.[.[nand false] false] -> true
.[.[nand false] true]  -> true
.[.[nand true]  false] -> true
.[.[nand true]  true]  -> false

.[not x_] -> .[.[nand x_] x_]

.[.[and x_] y_] -> .[not .[.[nand x_] y_]]

.[.[or x_] y_] -> .[.[nand .[.[nand x_] x_]] .[.[nand y_] y_]]

.[.[cond p_] q_] -> .[.[or .[not p_]] q_]

.[.[bicond p_] q_] -> .[.[and .[.[cond p_] q_]] .[.[cond q_] p_]]

# remember that order matters
.[.[eq x_] x_] -> true
.[.[eq x_] y_] -> false


